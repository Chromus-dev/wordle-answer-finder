{
	"name": "wordle-answer-finder",
	"version": "1.0.0",
	"description": "Answer finder for Wordle",
	"main": "index.js",
	"scripts": {
		"console": "node console.js",
		"start": "node web/app.js",
		"prod": "npx nodemon web/app.js",
		"package": "rm -rf bin && npx pkg console.js --output \"bin/Wordle Answer Finder\" --targets node18-linux-arm64,node18-macos-arm64,node18-win-arm64"
	},
	"author": "Chase",
	"license": "ISC",
	"dependencies": {
		"chalk": "^4.1.2",
		"express": "^4.18.2",
		"readline-sync": "^1.4.10"
	},
	"devDependencies": {
		"@types/readline-sync": "^1.4.4",
		"nodemon": "^2.0.22",
		"pkg": "^5.8.0"
	},
	"pkg": {
		"targets": [
			"node18-linux-arm64",
			"node18-macos-arm64",
			"node18-win-arm64"
		],
		"output": "bin/Wordle Answer Finder"
	}
}
